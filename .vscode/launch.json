  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "List Operators",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/run",
      "args": ["list-operators", "../test_reports/full_formulary.doc.json.pdf"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Validate Content",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/run",
      "args": [
        "validate-content",
        "../test_reports/full_formulary.doc.json.pdf"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}
